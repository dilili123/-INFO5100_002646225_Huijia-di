<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="300.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FinalProject.controller.FormatController">
   <children>
      <Text fill="#f24c8c" layoutX="77.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please choose a format below">
         <font>
            <Font name="Apple Braille Outline 6 Dot" size="22.0" />
         </font>
      </Text>
      <VBox layoutX="188.0" layoutY="87.0" prefHeight="132.0" prefWidth="100.0" spacing="15.0">
         <children>
            <RadioButton id="ButtonJPG" fx:id="ButtonJPG" mnemonicParsing="false" text="JPG" textFill="#299df0" />
            <RadioButton id="ButtonJPEG" fx:id="ButtonJPEG" mnemonicParsing="false" text="JPEG" textFill="#299df0" />
            <RadioButton id="ButtonPNG" fx:id="ButtonPNG" mnemonicParsing="false" text="PNG" textFill="#299df0" />
            <RadioButton id="ButtonGIF" fx:id="ButtonGIF" mnemonicParsing="false" text="GIF" textFill="#299df0" />
         </children>
      </VBox>
      <Button alignment="CENTER" layoutX="106.0" layoutY="236.0" mnemonicParsing="false" onAction="#handelOK" prefHeight="30.0" prefWidth="65.0" text="OK" textFill="#f24c8c" />
      <Button layoutX="277.0" layoutY="236.0" lineSpacing="30.0" mnemonicParsing="false" onAction="#handelCancel" prefHeight="30.0" prefWidth="65.0" text="Cancel" textFill="#f24c8c" />
   </children>
</AnchorPane>
